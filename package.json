{
    "name": "vscode-structurizr",
    "displayName": "vscode-structurizr",
    "description": "Structurizr Syntax Highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "structurizr",
            "aliases": ["Structurizr", "structurizr"],
            "extensions": ["sz"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "structurizr",
            "scopeName": "source.structurizr",
            "path": "./syntaxes/structurizr.tmLanguage.json"
        }]
    }
}